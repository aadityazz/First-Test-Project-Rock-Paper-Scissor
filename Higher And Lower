"""

# =======================================================================================================================

print(logo)
hol = ' '
p = 0
l1=''
l2=''
l1=random.choice(data)
l2=random.choice(data)

a=list(l1.keys())
b=list(l2.keys())

print(l1.keys())
print(vs)
print(l2.keys())
hol = input("Type H for high and L for lower")

while (hol == "h") or (hol == "l"):
    while hol == "h":
        if (list(l1.values())) > (list(l2.values())):
            print("you got a point")
            l2.clear()
            l2 = random.choice(data)
            p = +1
            a = l1.keys()
            b = l2.keys()
            print(l1.keys())
            print(vs)
            print(l2.keys())
            hol = input("Type H for high and L for lower")
        else:
            print("you loose")
            hol = ' '
    while hol == "l":
        if (list(l1.values())) < (list(l2.values())):
            l1.clear()
            l1 = random.choice(data)
            print("you got a point")
            p = +1
            a = l1.keys()
            b = l2.keys()
            print(l1.keys())
            print(vs)
            print(l2.keys())
            hol = input("Type H for high and L for lower")
        else:
            print("you loose")
            hol = ' '
    else:
        hol = ' '

print("your total points are", p)

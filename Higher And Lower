import random

data = [
    {'Instagram': 346}, {'Cristiano Ronaldo': 215}, {'Ariana Grande': 183}, {'Dwayne Johnson': 181},
    {'Selena Gomez': 174},
    {'Kylie Jenner': 172}, {'Kim Kardashian': 167}, {'Lionel Messi': 149}, {'BeyoncÃ©': 145}, {'Neymar': 138},
    {'National Geographic': 135}, {'Justin Bieber': 133}, {'Taylor Swift': 131}, {'Nike': 109}, {'Shakira': 66},
    {'Drake': 65}, {'Vin Diesel': 62}, {'Priyanka Chopra Jonas': 53}, {'9GAG': 52}
]

# ======================================================================================================================


logo = """
    __  ___       __             
   / / / (_)___ _/ /_  ___  _____
  / /_/ / / __ `/ __ \/ _ \/ ___/
 / __  / / /_/ / / / /  __/ /    

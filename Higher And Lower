| |/ (__  ) 
|___/____(_)
"""

# =======================================================================================================================

print(logo)
hol = ' '
p = 0
l = []
for i in range(0, 2):
    l.append((random.choice((data))))


def highlow():
    global hol, p, l, data
    if hol == "h":
        global hol, p, l, data
        if l[0].values() > l[1].values():
            print("you got a point")
            l.remove(l[1])
            l.append(l[0])
            p = +1
            print(l[0].keys())
            print(vs)
            print(l[1].keys())
            hol = input("Type H for high and L for lower")
            highlow()
        else:
            print("you loose")
    elif hol == "l":
        global hol, p, l, data
        if l[0].values() < l[1].values():
            l.remove(l[1])
            l.append(l[0])
            print("you got a point")
            p = +1
            print(l[0].keys())
            print(vs)
            print(l[1].keys())
            hol = input("Type H for high and L for lower")
            highlow()
        else:
            print("you loose")
    else:
        None

highlow()
